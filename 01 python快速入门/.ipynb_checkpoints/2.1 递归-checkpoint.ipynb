   "source": [
    "def Recursive(j):\n",
    "    sum_value = 0\n",
    "    if j == 0:\n",
    "        sum_value = 1\n",
    "    else:\n",
    "        sum_value = j * Recursive(j-1)\n",
    "    return sum_value"
   ]
